            </textField>
            <textField>
                <reportElement x = "300" y = "0" width = "136" height = "15" />
                <textElement textAlignment = "Center" verticalAlignment = "Middle" />
                <textFieldExpression class = "java.lang.String">
                    <![CDATA[$F{sampleId}]]>
                </textFieldExpression>
            </textField>
            <textField>
                <reportElement x = "400" y = "0" width = "136" height = "15" />
                <textElement textAlignment = "Center" verticalAlignment = "Middle" />
                <textFieldExpression class = "java.lang.String">
                    <![CDATA[$F{matchType}]]>
